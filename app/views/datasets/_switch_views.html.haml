- if policy(Dataset).public_view? && program_dataset_categories.length > 1
  .btn-group.btn-group-sm{"aria-label" => "", :role => "group"}
    - program_dataset_categories.each do |category|
      = link_to category.name, category_datasets_path(category), class: ['btn btn-outline-secondary', ('active' if category.id == params[:category_id])]
