- if policy(Dataset).public_view?
  = render "shared/scorecard_setting_tab_nav"

  - if program_dataset_categories.length > 1
    .btn-group.mt-2{"aria-label" => "Basic example", :role => "group", style: 'margin: 0 auto'}
      - program_dataset_categories.each do |category|
        = link_to category.name, category_datasets_path(category), class: ['btn btn-outline-secondary', ('active' if category.id == params[:category_id])]