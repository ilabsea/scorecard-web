%tr
  %td= activity_log.controller_name
  %td= activity_log.action_name
  %td= activity_log.http_format
  %td= activity_log.http_method
  %td.fixed-gap
    = activity_log.path
  %td
    = activity_log.http_status
  %td.fixed-gap
    = activity_log.payload
  %td.w-100
    = l activity_log.created_at, format: :long